<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <!--[if IE]>
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <![endif]-->
    <title>@ViewBag.Title - ResolveGram </title>
    @using ResolveGram.Helper;
    <!-- BOOTSTRAP CORE STYLE  -->
    <link href="~/Content/assets/css/bootstrap.css" rel="stylesheet" />
    <!-- FONT AWESOME ICONS  -->
    <link href="~/Content/assets/css/font-awesome.css" rel="stylesheet" />
    <!-- CUSTOM STYLE  -->
    <link href="~/Content/assets/css/style.css" rel="stylesheet" />
    <!-- HTML5 Shiv and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <header>
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <span class="pull-left">
                        @Html.Partial("AuthPartial")
                    </span>
                    <strong>Email: </strong>support@resolvegram.com
                    &nbsp;&nbsp;
                    <strong>Support: </strong>+234 8095300641
                </div>

            </div>
        </div>
    </header>
    <!-- HEADER END-->
    <div class="navbar navbar-inverse set-radius-zero">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="/Home/Dashboard">

                    <img src="~/Content/assets/img/logo.png" />
                </a>

            </div>

            <div class="left-div" style="padding: 10px;">
                <div class="user-settings-wrapper">

                </div>
            </div>
        </div>
    </div>
    <!-- LOGO HEADER END-->
    <section class="menu-section">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="navbar-collapse collapse ">
                        <ul id="menu-top" class="nav navbar-nav navbar-right">
     
                            <li>@Html.MenuLink("Dashboard", "Dashboard", "Home")</li>
                            <li>@Html.MenuLink("Tasks", "AllTask", "Home")</li>
                            <li>@Html.MenuLink("Settings", "Settings", "Home")</li>
                            <li>@Html.MenuLink("Account", "Account", "Home")</li>
                            <li>@Html.MenuLink("Report", "Report", "Home")</li>
                            <li>@Html.MenuLink("Support", "Contact", "Home")</li>
                        </ul>
                    </div>
                </div>

            </div>
        </div>
    </section>
    <!-- MENU SECTION END-->
    <div class="content-wrapper">
        <div class="container">
            @RenderBody()
            <div class="row">
                @*<div class="col-md-12">
                        <div class="alert alert-success">

                            <div class="text-center alert alert-warning">
                                <a href="#" class="btn btn-social btn-facebook">
                                    <i class="fa fa-facebook"></i>&nbsp; Facebook
                                </a>
                                <a href="#" class="btn btn-social btn-google">
                                    <i class="fa fa-google-plus"></i>&nbsp; Google
                                </a>
                                <a href="#" class="btn btn-social btn-twitter">
                                    <i class="fa fa-twitter"></i>&nbsp; Twitter
                                </a>
                                <a href="#" class="btn btn-social btn-linkedin">
                                    <i class="fa fa-linkedin"></i>&nbsp; Linkedin
                                </a>
                            </div>
                        </div>
                    </div>*@

            </div>
        </div>
    </div>
    <!-- CONTENT-WRAPPER SECTION END-->
    <footer>
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    &copy; 2018 <a href="http://www.resolvegram.com">ResolveGram</a> | Theme by : <a href="http://www.designbootstrap.com/" target="_blank">Design Bootstrap</a>
                </div>

            </div>
        </div>
    </footer>
    <!-- FOOTER SECTION END-->
    <!-- JAVASCRIPT AT THE BOTTOM TO REDUCE THE LOADING TIME  -->
    <!-- CORE JQUERY SCRIPTS -->
    <script src="~/Content/assets/js/jquery-1.11.1.js"></script>
    <!-- BOOTSTRAP SCRIPTS  -->
    <script src="~/Content/assets/js/bootstrap.js"></script>
</body>
</html>
